{
  "name": "schaeffler-frontend",
  "version": "0.2.0",
  "license": "MIT",
  "repository": {
    "url": "https://gitlab.schaeffler.com/frontend-schaeffler/schaeffler-frontend.git"
  },
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build --head=HEAD",
    "affected:e2e": "nx affected:e2e --head=HEAD",
    "affected:e2e:headless": "npm run affected:e2e -- --headless",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "lint:html": "npm run lint:html-apps && npm run lint:html-libs",
    "lint:html-apps": "htmlhint apps/**/*.html",
    "lint:html-libs": "htmlhint libs/**/*.html",
    "lint:scss": "npm run lint:scss-apps && npm run lint:scss-libs",
    "lint:scss-apps": "stylelint ./apps/**/*.scss --config .stylelintrc --allow-empty-input",
    "lint:scss-libs": "stylelint ./libs/**/*.scss --config .stylelintrc --allow-empty-input",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "generate-readme": "md-magic --path ./docs/*_*.md --config ./markdown.js",
    "cm": "pretty-quick --staged && git-cz --no-verify",
    "release": "standard-version --no-verify",
    "postinstall": "npm run ngcc && npm run generate-readme",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS || (echo You can use 'npm run cm' for CLI support when creating conventional commits && exit 1)"
    }
  },
  "lint-staged": {
    "**/*/assets/i18n/*.json": [
      "transloco-validator"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.2",
    "@angular/cdk": "9.0.1",
    "@angular/common": "9.0.2",
    "@angular/compiler": "9.0.2",
    "@angular/core": "9.0.2",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.0.2",
    "@angular/material": "9.0.1",
    "@angular/platform-browser": "9.0.2",
    "@angular/platform-browser-dynamic": "9.0.2",
    "@angular/router": "9.0.2",
    "@ngneat/transloco": "2.13.5",
    "@ngrx/effects": "8.6.0",
    "@ngrx/router-store": "8.6.0",
    "@ngrx/store": "8.6.0",
    "@types/jwt-decode": "2.2.1",
    "angular-oauth2-oidc": "8.0.4",
    "core-js": "3.6.4",
    "devextreme": "19.2.6",
    "devextreme-angular": "19.2.6",
    "hammerjs": "2.0.8",
    "jwt-decode": "2.2.0",
    "keycloak-angular": "7.1.0",
    "keycloak-js": "8.0.2",
    "ngx-papaparse": "4.0.4",
    "rxjs": "6.5.4",
    "schaeffler-icons": "1.1.1",
    "tslib": "1.11.1",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-ng-packagr": "0.900.5",
    "@angular-devkit/build-angular": "0.900.5",
    "@angular/cli": "9.0.3",
    "@angular/compiler-cli": "9.0.2",
    "@angular/language-service": "9.0.2",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@ngneat/transloco-keys-manager": "2.0.2",
    "@ngneat/transloco-optimize": "1.0.1",
    "@ngneat/transloco-validator": "1.0.0",
    "@ngrx/store-devtools": "8.6.0",
    "@nrwl/angular": "9.0.2",
    "@nrwl/cli": "9.0.2",
    "@nrwl/cypress": "9.0.2",
    "@nrwl/jest": "9.0.2",
    "@nrwl/schematics": "8.12.7",
    "@nrwl/workspace": "9.0.2",
    "@tableau/extensions-api-types": "1.3.1",
    "@testing-library/jest-dom": "5.1.1",
    "@types/jest": "25.1.3",
    "@types/node": "13.7.7",
    "angular-tslint-rules": "1.20.3",
    "codelyzer": "5.2.1",
    "commitizen": "4.0.3",
    "cypress": "4.1.0",
    "cz-conventional-changelog": "3.1.0",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "git-branch": "2.0.1",
    "htmlhint": "0.11.0",
    "husky": "4.2.3",
    "jasmine-marbles": "0.6.0",
    "jest": "25.1.0",
    "jest-canvas-mock": "2.2.0",
    "jest-junit": "10.0.0",
    "jest-marbles": "2.5.1",
    "jest-preset-angular": "8.0.0",
    "lint-staged": "10.0.8",
    "markdown-include": "0.4.3",
    "markdown-magic": "1.0.0",
    "markdown-magic-version-badge": "1.1.1",
    "ng-bullet": "1.0.3",
    "ng-packagr": "9.0.2",
    "ngx-build-plus": "9.0.6",
    "npm-audit-html": "1.4.0",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.1",
    "standard-version": "7.1.0",
    "stylelint": "13.2.0",
    "stylelint-order": "4.0.0",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.5",
    "webpack-bundle-analyzer": "3.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
