{
  "name": "schaeffler-frontend",
  "version": "1.4.0",
  "license": "MIT",
  "repository": {
    "url": "https://gitlab.schaeffler.com/frontend-schaeffler/schaeffler-frontend.git"
  },
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:debug": "node ./tools/npm-scripts/debug-unit-tests.js",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build --head=HEAD",
    "affected:e2e": "nx affected:e2e --head=HEAD --exclude=cdba-e2e,helloworld-azure-e2e,sta-e2e,ltp-e2e,seli-e2e,goldwind-e2e --configuration=e2e",
    "affected:e2e:headless": "npm run affected:e2e -- --headless",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint --parallel",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "lint:html": "npm run lint:html-apps && npm run lint:html-libs",
    "lint:html-apps": "htmlhint apps/**/*.html",
    "lint:html-libs": "htmlhint libs/**/*.html",
    "lint:scss": "npm run lint:scss-apps && npm run lint:scss-libs",
    "lint:scss-apps": "stylelint ./apps/**/*.scss --config .stylelintrc --allow-empty-input",
    "lint:scss-libs": "stylelint ./libs/**/*.scss --config .stylelintrc --allow-empty-input",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "generate-readme": "md-magic --path ./docs/*_*.md --config ./markdown.js",
    "cm": "pretty-quick --staged && git-cz --no-verify",
    "release": "standard-version --no-verify",
    "postinstall": "npm run ngcc && npm run generate-readme && npm run version && node ./decorate-angular-cli.js",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "version": "cross-var node ./tools/npm-scripts/version.js $npm_package_version",
    "clear_jest": "jest --clearCache",
    "transloco:optimize": "transloco-optimize"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS || (echo You can use 'npm run cm' for CLI support when creating conventional commits && exit 1)"
    }
  },
  "lint-staged": {
    "**/*/assets/i18n/*.json": [
      "transloco-validator"
    ]
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/angular": "23.2.1",
    "@ag-grid-community/client-side-row-model": "23.2.1",
    "@ag-grid-community/core": "23.2.1",
    "@ag-grid-enterprise/clipboard": "23.2.1",
    "@ag-grid-enterprise/column-tool-panel": "23.2.1",
    "@ag-grid-enterprise/core": "23.2.1",
    "@ag-grid-enterprise/filter-tool-panel": "23.2.1",
    "@ag-grid-enterprise/menu": "23.2.1",
    "@ag-grid-enterprise/range-selection": "23.2.1",
    "@ag-grid-enterprise/row-grouping": "23.2.1",
    "@ag-grid-enterprise/set-filter": "23.2.1",
    "@ag-grid-enterprise/side-bar": "23.2.1",
    "@ag-grid-enterprise/status-bar": "23.2.1",
    "@angular/animations": "10.0.12",
    "@angular/cdk": "10.1.3",
    "@angular/common": "10.0.12",
    "@angular/compiler": "10.0.12",
    "@angular/core": "10.0.12",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "10.0.12",
    "@angular/material": "10.1.3",
    "@angular/platform-browser": "10.0.12",
    "@angular/platform-browser-dynamic": "10.0.12",
    "@angular/router": "10.0.12",
    "@ngneat/transloco": "2.19.1",
    "@ngrx/component": "10.0.0",
    "@ngrx/effects": "10.0.0",
    "@ngrx/entity": "10.0.0",
    "@ngrx/router-store": "10.0.0",
    "@ngrx/store": "10.0.0",
    "@stomp/rx-stomp": "0.3.5",
    "@stomp/stompjs": "5.4.4",
    "@types/echarts": "4.6.4",
    "@types/jwt-decode": "2.2.1",
    "angular-oauth2-oidc": "10.0.3",
    "angular-oauth2-oidc-jwks": "9.0.0",
    "core-js": "3.6.4",
    "devextreme": "20.1.6",
    "devextreme-angular": "20.1.6",
    "echarts": "4.8.0",
    "jwt-decode": "2.2.0",
    "ngx-echarts": "5.1.2",
    "ngx-papaparse": "4.0.4",
    "rxjs": "6.6.2",
    "schaeffler-icons": "1.1.1",
    "tslib": "2.0.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.7",
    "@angular-devkit/build-ng-packagr": "0.1000.7",
    "@angular-devkit/core": "10.0.7",
    "@angular-devkit/schematics": "10.0.7",
    "@angular/cli": "10.0.7",
    "@angular/compiler-cli": "10.0.12",
    "@angular/language-service": "10.0.12",
    "@babel/core": "7.11.4",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@ngneat/transloco-keys-manager": "2.2.1",
    "@ngneat/transloco-optimize": "1.0.2",
    "@ngneat/transloco-validator": "1.0.1",
    "@ngrx/store-devtools": "10.0.0",
    "@nrwl/angular": "10.1.0",
    "@nrwl/cli": "10.1.0",
    "@nrwl/cypress": "10.1.0",
    "@nrwl/jest": "10.1.0",
    "@nrwl/storybook": "10.1.0",
    "@nrwl/workspace": "10.1.0",
    "@storybook/addon-a11y": "6.0.18",
    "@storybook/addon-actions": "6.0.18",
    "@storybook/addon-backgrounds": "6.0.18",
    "@storybook/addon-knobs": "6.0.18",
    "@storybook/addon-notes": "5.3.19",
    "@storybook/addon-storysource": "6.0.18",
    "@storybook/addon-viewport": "6.0.18",
    "@storybook/angular": "6.0.18",
    "@testing-library/jest-dom": "5.11.4",
    "@types/jest": "26.0.10",
    "@types/node": "13.13.15",
    "angular-tslint-rules": "1.20.4",
    "babel-loader": "8.1.0",
    "codelyzer": "6.0.0",
    "commitizen": "4.2.1",
    "cross-var": "1.1.0",
    "cypress": "5.0.0",
    "cz-conventional-changelog": "3.2.1",
    "dotenv": "8.2.0",
    "eslint": "7.7.0",
    "eslint-plugin-cypress": "2.11.1",
    "git-branch": "2.0.1",
    "htmlhint": "0.14.1",
    "husky": "4.2.5",
    "jasmine-marbles": "0.6.0",
    "jest": "26.4.2",
    "jest-canvas-mock": "2.2.0",
    "jest-junit": "11.1.0",
    "jest-marbles": "2.5.1",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "10.2.13",
    "markdown-include": "0.4.3",
    "markdown-magic": "1.0.0",
    "markdown-magic-version-badge": "1.1.1",
    "ng-bullet": "1.0.3",
    "ng-packagr": "10.0.4",
    "ngx-build-plus": "10.1.1",
    "npm-audit-html": "1.4.3",
    "prettier": "2.0.5",
    "pretty-quick": "3.0.0",
    "standard-version": "9.0.0",
    "stylelint": "13.6.1",
    "stylelint-order": "4.1.0",
    "ts-jest": "26.2.0",
    "ts-node": "9.0.0",
    "tsickle": "0.39.1",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.9.7",
    "webpack-bundle-analyzer": "3.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
