  "private": "true",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "prepare": "husky install",
    "postinstall": "pnpm run generate-readme && node ./decorate-angular-cli.js",
    "commit": "pretty-quick --staged && git-cz",
    "affected": "nx affected",
    "dep-graph": "nx dep-graph",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e --head=HEAD --exclude=shared-ui-storybook-e2e --parallel=1 --configuration=e2e",
    "lint:html": "npm run lint:html-apps && npm run lint:html-libs",
    "lint:html-apps": "htmlhint apps/**/*.html",
    "lint:html-libs": "htmlhint libs/**/*.html",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "generate-changelog": "node ./tools/npm-scripts/changelog.js",
    "generate-readme": "md-magic --path './docs/*_*.md'",
    "embed-app:iframe": "http-server tools/embedded/iframe",
    "embed-app:webcomponent": "http-server tools/embedded/webcomponent",
    "embed-app:ea": "npm run embed-app:webcomponent & nx run ea:build:local --watch & http-server --cors --c-1 --port 8000 dist/apps/ea",
    "help": "nx help",
    "renovate": "renovate"
  },
  "lint-staged": {
    "**/*/assets/i18n/*.json": [
      "transloco-validator"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
