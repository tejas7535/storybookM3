{
  "name": "schaeffler-frontend",
  "version": "1.36.0",
  "license": "MIT",
  "repository": {
    "url": "https://gitlab.schaeffler.com/frontend-schaeffler/schaeffler-frontend.git"
  },
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:debug": "node ./tools/npm-scripts/debug-unit-tests.js",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build --head=HEAD",
    "affected:e2e": "nx affected:e2e --head=HEAD --exclude=cdba-e2e,helloworld-azure-e2e,sta-e2e,ltp-e2e,seli-e2e,goldwind-e2e,gq-e2e,mac-e2e,ia-e2e,kitchen-sink-e2e --configuration=e2e",
    "affected:e2e:headless": "npm run affected:e2e -- --headless",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint --parallel",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "lint:html": "npm run lint:html-apps && npm run lint:html-libs",
    "lint:html-apps": "htmlhint apps/**/*.html",
    "lint:html-libs": "htmlhint libs/**/*.html",
    "lint:scss": "npm run lint:scss-apps && npm run lint:scss-libs",
    "lint:scss-apps": "stylelint ./apps/**/*.scss --config .stylelintrc --allow-empty-input",
    "lint:scss-libs": "stylelint ./libs/**/*.scss --config .stylelintrc --allow-empty-input",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "generate-readme": "md-magic --path ./docs/*_*.md --config ./markdown.js",
    "generate-changelog": "node ./tools/npm-scripts/changelog.js",
    "cm": "pretty-quick --staged && git-cz --no-verify",
    "release": "standard-version --no-verify",
    "force-resolutions": "npx npm-force-resolutions && npx nx format:write --files=package-lock.json",
    "preinstall": "node ./tools/npm-scripts/preinstall.js",
    "postinstall": "npm run ngcc && npm run generate-readme && node ./decorate-angular-cli.js",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "clear_jest": "jest --clearCache",
    "transloco:optimize": "transloco-optimize"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS || (echo You can use 'npm run cm' for CLI support when creating conventional commits && exit 1)"
    }
  },
  "lint-staged": {
    "**/*/assets/i18n/*.json": [
      "transloco-validator"
    ]
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/all-modules": "24.1.0",
    "@ag-grid-community/angular": "24.1.0",
    "@ag-grid-enterprise/all-modules": "24.1.0",
    "@angular/animations": "11.1.0",
    "@angular/cdk": "11.1.0",
    "@angular/common": "11.1.0",
    "@angular/compiler": "11.1.0",
    "@angular/core": "11.1.0",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.1.0",
    "@angular/material": "11.1.0",
    "@angular/platform-browser": "11.1.0",
    "@angular/platform-browser-dynamic": "11.1.0",
    "@angular/router": "11.1.0",
    "@microsoft/applicationinsights-web": "2.5.11",
    "@ngneat/transloco": "2.20.0",
    "@ngrx/component": "10.1.2",
    "@ngrx/effects": "10.1.2",
    "@ngrx/entity": "10.1.2",
    "@ngrx/router-store": "10.1.2",
    "@ngrx/store": "10.1.2",
    "angular-oauth2-oidc": "10.0.3",
    "angular-oauth2-oidc-jwks": "9.0.0",
    "core-js": "3.8.3",
    "d3-org-chart": "1.1.3",
    "echarts": "5.0.1",
    "jwt-decode": "3.1.2",
    "ngx-echarts": "6.0.0",
    "ngx-papaparse": "5.0.0",
    "rxjs": "6.6.3",
    "schaeffler-icons": "1.1.1",
    "tslib": "2.1.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.7",
    "@angular-devkit/core": "11.0.7",
    "@angular-devkit/schematics": "11.0.7",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/cli": "11.0.7",
    "@angular/compiler-cli": "11.1.0",
    "@angular/language-service": "11.1.0",
    "@babel/core": "7.12.10",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@ngneat/spectator": "6.1.3",
    "@ngneat/transloco-keys-manager": "2.7.1",
    "@ngneat/transloco-optimize": "1.0.2",
    "@ngneat/transloco-validator": "1.0.1",
    "@ngrx/store-devtools": "10.1.2",
    "@nrwl/angular": "11.1.5",
    "@nrwl/cli": "11.1.5",
    "@nrwl/cypress": "11.1.5",
    "@nrwl/eslint-plugin-nx": "11.1.5",
    "@nrwl/jest": "11.1.5",
    "@nrwl/storybook": "11.1.5",
    "@nrwl/workspace": "11.1.5",
    "@storybook/addon-a11y": "6.1.14",
    "@storybook/addon-actions": "6.1.14",
    "@storybook/addon-backgrounds": "6.1.14",
    "@storybook/addon-knobs": "6.1.14",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addon-storysource": "6.1.14",
    "@storybook/addon-viewport": "6.1.14",
    "@storybook/angular": "6.1.14",
    "@testing-library/jest-dom": "5.11.9",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "angular-tslint-rules": "1.20.4",
    "babel-loader": "8.2.2",
    "codelyzer": "6.0.1",
    "commitizen": "4.2.3",
    "cross-var": "1.1.0",
    "cypress": "6.3.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "8.2.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-cypress": "2.11.2",
    "htmlhint": "0.14.2",
    "husky": "4.3.8",
    "jasmine-marbles": "0.6.0",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.0",
    "jest-junit": "12.0.0",
    "jest-marbles": "2.5.1",
    "jest-preset-angular": "8.3.2",
    "lint-staged": "10.5.3",
    "markdown-include": "0.4.3",
    "markdown-magic": "1.0.0",
    "markdown-magic-version-badge": "1.1.1",
    "ng-bullet": "1.0.3",
    "ng-packagr": "11.0.3",
    "ngx-build-plus": "11.0.0",
    "npm-audit-html": "1.4.3",
    "npm-force-resolutions": "0.0.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "standard-version": "9.1.0",
    "stylelint": "13.9.0",
    "stylelint-order": "4.1.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "tsickle": "0.39.1",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.0.5",
    "webpack-bundle-analyzer": "4.4.0",
    "yargs": "16.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "resolutions": {
    "@angular-devkit/schematics": "11.0.7"
  }
}
