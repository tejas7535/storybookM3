{
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "package-lock.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*",
    "Jenkinsfile": "*",
    "deployments.json": "*",
    "tsconfig.base.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "schaeffler",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ],
        "parallel": 1
      }
    },
    "ci": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ],
        "cacheDirectory": "/home/adp-jenkins/temp/nx-cache"
      }
    }
  },
  "targetDependencies": {
    "storybook": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "build-storybook": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "cli": {
    "cache": {
      "enabled": true,
      "environment": "all"
    }
  },
  "defaultProject": "helloworld-azure",
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@ngneat/spectator:spectator-component": {
      "jest": true,
      "style": "scss"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    }
  }
}
