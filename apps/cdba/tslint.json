{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [true, "attribute", "cdba", "camelCase"],
    "component-selector": [true, "element", "cdba", "kebab-case"],
    "template-cyclomatic-complexity": [true, 15],
    "no-implicit-dependencies": false,
    "no-default-import": false
  },
  "linterOptions": {
    "exclude": ["!**/*"]
  }
}
