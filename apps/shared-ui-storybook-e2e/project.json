{
  "name": "shared-ui-storybook-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/shared-ui-storybook-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/shared-ui-storybook-e2e/cypress.config.ts",
        "devServerTarget": "shared-ui-storybook:storybook",
        "testingType": "e2e"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "shared-ui-storybook:storybook:ci"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "configurations": {
        "ci": {
          "outputs": ["{workspaceRoot}/eslint/{projectRoot}"],
          "format": "checkstyle",
          "outputFile": "checkstyle/apps/shared-ui-storybook-e2e/checkstyle.xml",
          "force": true
        }
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["shared-ui-storybook"]
}
