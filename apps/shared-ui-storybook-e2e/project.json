{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/shared-ui-storybook-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/shared-ui-storybook-e2e/cypress.json",
        "tsConfig": "apps/shared-ui-storybook-e2e/tsconfig.e2e.json",
        "devServerTarget": "shared-ui-storybook:storybook"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "shared-ui-storybook:storybook:ci"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/shared-ui-storybook-e2e/src/**/*.ts",
          "apps/shared-ui-storybook-e2e/src/**/*.html"
        ]
      },
      "configurations": {
        "ci": {
          "format": "checkstyle",
          "outputFile": "checkstyle/apps/shared-ui-storybook-e2e/checkstyle.xml",
          "force": true
        }
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "push": true,
        "preset": "conventional",
        "baseBranch": "master",
        "tagPrefix": "${target}-v",
        "changelogHeader": "# Release Notes of Storybook",
        "commitMessageFormat": "chore(${projectName}): ðŸš€ release version ${version} [CI SKIP]",
        "noVerify": true,
        "postTargets": ["shared-ui-storybook-e2e:github"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["shared-ui-storybook"]
}
