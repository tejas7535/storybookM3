{
  "name": "ga-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ga-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/ga-e2e/cypress.config.ts",
        "devServerTarget": "ga:serve:e2e",
        "testingType": "e2e"
      },
      "configurations": {
        "local": {
          "devServerTarget": "ga:serve:local"
        },
        "production": {
          "devServerTarget": "ga:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "configurations": {
        "ci": {
          "format": "checkstyle",
          "outputFile": "checkstyle/apps/ga-e2e/checkstyle.xml",
          "force": true
        }
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["ga"]
}
