{
  "name": "ea",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {
    "@nx/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/ea/src",
  "prefix": "ea",
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ea",
        "index": "apps/ea/src/index.html",
        "main": "apps/ea/src/main.ts",
        "polyfills": "apps/ea/src/polyfills.ts",
        "tsConfig": "apps/ea/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/ea/src/favicon.ico",
          "apps/ea/src/assets",
          "apps/ea/src/browserconfig.xml",
          "apps/ea/src/robots.txt"
        ],
        "styles": [
          "apps/ea/src/styles.scss",
          "node_modules/@fontsource/material-icons/400.css",
          "node_modules/@fontsource/noto-sans/400.css",
          "node_modules/@fontsource/noto-sans/500.css"
        ],
        "scripts": [],
        "budgets": [
          {
            "type": "initial",
            "maximumWarning": "2mb",
            "maximumError": "6mb"
          },
          {
            "type": "anyComponentStyle",
            "maximumWarning": "6kb",
            "maximumError": "10kb"
          }
        ],
        "buildLibsFromSource": false
      },
      "configurations": {
        "default": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "assets": [
            "apps/ea/src/favicon.ico",
            "apps/ea/src/assets",
            "apps/ea/src/browserconfig.xml",
            {
              "input": "apps/ea/robots/dev",
              "output": "/",
              "glob": "robots.txt"
            }
          ]
        },
        "local": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "deployUrl": "http://localhost:8000/",
          "fileReplacements": [
            {
              "replace": "apps/ea/src/environments/environment.ts",
              "with": "apps/ea/src/environments/environment.local.ts"
            }
          ],
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "9000mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "assets": [
            "apps/ea/src/favicon.ico",
            "apps/ea/src/assets",
            "apps/ea/src/browserconfig.xml",
            {
              "input": "apps/ea/robots/dev",
              "output": "/",
              "glob": "robots.txt"
            },
            {
              "input": "apps/ea/webcomponent-bundle",
              "output": "/",
              "glob": "ea-webcomponent.js"
            }
          ]
        },
        "dev": {
          "statsJson": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "optimization": {
            "scripts": true,
            "styles": {
              "minify": true,
              "inlineCritical": false
            },
            "fonts": false
          },
          "fileReplacements": [
            {
              "replace": "apps/ea/src/environments/environment.ts",
              "with": "apps/ea/src/environments/environment.dev.ts"
            }
          ],
          "assets": [
            "apps/ea/src/favicon.ico",
            "apps/ea/src/assets",
            "apps/ea/src/browserconfig.xml",
            {
              "input": "apps/ea/robots/dev",
              "output": "/",
              "glob": "robots.txt"
            }
          ]
        },
        "qa": {
          "outputHashing": "bundles",
          "statsJson": true,
          "optimization": {
            "scripts": true,
            "styles": {
              "minify": true,
              "inlineCritical": false
            },
            "fonts": false
          },
          "fileReplacements": [
            {
              "replace": "apps/ea/src/environments/environment.ts",
              "with": "apps/ea/src/environments/environment.qa.ts"
            }
          ],
          "assets": [
            "apps/ea/src/favicon.ico",
            "apps/ea/src/assets",
            "apps/ea/src/browserconfig.xml",
            {
              "input": "apps/ea/robots/dev",
              "output": "/",
              "glob": "robots.txt"
            }
          ]
        },
        "production": {
          "outputHashing": "bundles",
          "optimization": {
            "scripts": true,
            "styles": {
              "minify": true,
              "inlineCritical": false
            },
            "fonts": false
          },
          "fileReplacements": [
            {
              "replace": "apps/ea/src/environments/environment.ts",
              "with": "apps/ea/src/environments/environment.prod.ts"
            }
          ],
          "assets": [
            "apps/ea/src/favicon.ico",
            "apps/ea/src/assets",
            "apps/ea/src/browserconfig.xml",
            {
              "input": "apps/ea/robots/prod",
              "output": "/",
              "glob": "robots.txt"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "configurations": {
        "default": {
          "buildTarget": "ea:build:default"
        },
        "local": {
          "buildTarget": "ea:build:local"
        },
        "dev": {
          "buildTarget": "ea:build:dev"
        },
        "qa": {
          "buildTarget": "ea:build:qa"
        },
        "production": {
          "buildTarget": "ea:build:production"
        },
        "e2e": {
          "host": "0.0.0.0",
          "buildTarget": "ea:build:default"
        }
      },
      "defaultConfiguration": "default"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "configurations": {
        "ci": {
          "format": "json",
          "outputFile": "eslint/apps/ea/eslint.json",
          "force": true
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/ea"],
      "options": {
        "jestConfig": "apps/ea/jest.config.ts"
      }
    },
    "sonar": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "sonar-scanner -Dsonar.token=${SONAR_TOKEN} -Dproject.settings=apps/ea/sonar-project.properties"
        ],
        "parallel": false
      },
      "outputs": []
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "push": true,
        "preset": "conventional",
        "baseBranch": "master",
        "tagPrefix": "${target}-v",
        "changelogHeader": "# Release Notes of Engineering App",
        "commitMessageFormat": "chore({projectName}): ðŸ§ˆ release version ${version} [CI SKIP]",
        "noVerify": true,
        "postTargets": ["ea:github"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    },
    "zip": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          "mkdir -p dist/zips/ea",
          "cd dist/apps/ea && zip -r ../../zips/ea/next.zip *"
        ],
        "parallel": false
      }
    },
    "transloco-optimize": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "pnpm transloco-optimize dist/apps/ea/assets/i18n"
      }
    },
    "analyze-bundle": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/webpack/ea/bundle-report.html"],
      "options": {
        "command": "pnpm webpack-bundle-analyzer dist/apps/ea/stats.json --mode static --report dist/webpack/ea/bundle-report.html --title 'EA Bundle Report' --no-open || true"
      }
    }
  },
  "tags": []
}
