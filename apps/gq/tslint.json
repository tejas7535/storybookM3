{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [true, "attribute", "gq", "camelCase"],
    "component-selector": [true, "element", "gq", "kebab-case"],
    "template-cyclomatic-complexity": [true, 15],
    "no-default-import": false
  },
  "linterOptions": {
    "exclude": ["!**/*"]
  }
}
