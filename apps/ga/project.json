{
  "name": "ga",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {
    "@nx/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/ga/src",
  "prefix": "ga",
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ga",
        "index": "apps/ga/src/index.html",
        "main": "apps/ga/src/main.ts",
        "polyfills": "apps/ga/src/polyfills.ts",
        "tsConfig": "apps/ga/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/ga/src/favicon.ico",
          "apps/ga/src/assets",
          "apps/ga/src/browserconfig.xml",
          "apps/ga/src/robots.txt"
        ],
        "styles": [
          "apps/ga/src/styles.scss",
          "node_modules/@fontsource/material-icons/400.css",
          "node_modules/@fontsource/noto-sans/400.css",
          "node_modules/@fontsource/noto-sans/500.css"
        ],
        "scripts": [],
        "budgets": [
          {
            "type": "initial",
            "maximumWarning": "2mb",
            "maximumError": "7mb"
          },
          {
            "type": "anyComponentStyle",
            "maximumWarning": "6kb",
            "maximumError": "10kb"
          }
        ],
        "buildLibsFromSource": false
      },
      "configurations": {
        "local": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "assets": [
            "apps/ga/src/favicon.ico",
            "apps/ga/src/assets",
            "apps/ga/src/browserconfig.xml",
            {
              "input": "apps/ga/robots/dev",
              "output": "/",
              "glob": "robots.txt"
            }
          ]
        },
        "dev": {
          "statsJson": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/ga/src/environments/environment.ts",
              "with": "apps/ga/src/environments/environment.dev.ts"
            }
          ],
          "assets": [
            "apps/ga/src/favicon.ico",
            "apps/ga/src/assets",
            "apps/ga/src/browserconfig.xml",
            {
              "input": "apps/ga/robots/dev",
              "output": "/",
              "glob": "robots.txt"
            }
          ]
        },
        "qa": {
          "outputHashing": "bundles",
          "statsJson": true,
          "fileReplacements": [
            {
              "replace": "apps/ga/src/environments/environment.ts",
              "with": "apps/ga/src/environments/environment.qa.ts"
            }
          ],
          "assets": [
            "apps/ga/src/favicon.ico",
            "apps/ga/src/assets",
            "apps/ga/src/browserconfig.xml",
            {
              "input": "apps/ga/robots/dev",
              "output": "/",
              "glob": "robots.txt"
            }
          ]
        },
        "production": {
          "outputHashing": "bundles",
          "fileReplacements": [
            {
              "replace": "apps/ga/src/environments/environment.ts",
              "with": "apps/ga/src/environments/environment.prod.ts"
            }
          ],
          "assets": [
            "apps/ga/src/favicon.ico",
            "apps/ga/src/assets",
            "apps/ga/src/browserconfig.xml",
            {
              "input": "apps/ga/robots/prod",
              "output": "/",
              "glob": "robots.txt"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:webpack-dev-server",
      "configurations": {
        "local": {
          "browserTarget": "ga:build:local"
        },
        "dev": {
          "browserTarget": "ga:build:dev"
        },
        "qa": {
          "browserTarget": "ga:build:qa"
        },
        "production": {
          "browserTarget": "ga:build:production"
        },
        "e2e": {
          "browserTarget": "ga:build",
          "host": "0.0.0.0"
        }
      },
      "defaultConfiguration": "local"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/ga/src/**/*.ts", "apps/ga/src/**/*.html"]
      },
      "configurations": {
        "ci": {
          "format": "checkstyle",
          "outputFile": "checkstyle/apps/ga/checkstyle.xml",
          "force": true
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/ga"],
      "options": {
        "jestConfig": "apps/ga/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "push": true,
        "preset": "conventional",
        "baseBranch": "master",
        "tagPrefix": "${target}-v",
        "changelogHeader": "# Release Notes of Grease App",
        "commitMessageFormat": "chore(${projectName}): ðŸ§ˆ release version ${version} [CI SKIP]",
        "noVerify": true,
        "postTargets": ["ga:github"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    }
  },
  "tags": []
}
