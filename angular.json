{
  "version": 1,
  "projects": {
    "kitchen-sink": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/kitchen-sink",
      "sourceRoot": "apps/kitchen-sink/src",
      "prefix": "schaeffler",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/kitchen-sink",
            "index": "apps/kitchen-sink/src/index.html",
            "main": "apps/kitchen-sink/src/main.ts",
            "polyfills": "apps/kitchen-sink/src/polyfills.ts",
            "tsConfig": "apps/kitchen-sink/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/kitchen-sink/src/favicon.ico",
              "apps/kitchen-sink/src/assets"
            ],
            "styles": [
              "apps/kitchen-sink/src/styles.scss",
              "node_modules/schaeffler-icons/style.css"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/shared/styles/src"]
            }
          },
          "configurations": {
            "dev": {
              "statsJson": true,
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "apps/kitchen-sink/src/environments/environment.ts",
                  "with": "apps/kitchen-sink/src/environments/environment.dev.ts"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/kitchen-sink/src/environments/environment.ts",
                  "with": "apps/kitchen-sink/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kitchen-sink:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "kitchen-sink:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kitchen-sink:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/kitchen-sink/tsconfig.app.json",
              "apps/kitchen-sink/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/kitchen-sink/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/kitchen-sink/jest.config.js",
            "tsConfig": "apps/kitchen-sink/tsconfig.spec.json",
            "setupFile": "apps/kitchen-sink/src/test-setup.ts"
          }
        }
      }
    },
    "kitchen-sink-e2e": {
      "root": "apps/kitchen-sink-e2e",
