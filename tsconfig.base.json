{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "baseUrl": ".",
    "paths": {
      "@cdba/*": ["apps/cdba/src/app/*"],
      "@cdba/environments/*": ["apps/cdba/src/environments/*"],
      "@cdba/testing/*": ["apps/cdba/src/testing/*"],
      "@schaeffler/app-shell": ["libs/shared/ui/app-shell/src/index.ts"],
      "@schaeffler/application-insights": [
        "libs/shared/utils/application-insights/src/index.ts"
      ],
      "@schaeffler/azure-auth": ["libs/shared/utils/azure-auth/src/index.ts"],
      "@schaeffler/banner": ["libs/shared/ui/banner/src/index.ts"],
      "@schaeffler/breadcrumbs": ["libs/shared/ui/breadcrumbs/src/index.ts"],
      "@schaeffler/dropdown-input": [
        "libs/shared/ui/dropdown-input/src/index.ts"
      ],
      "@schaeffler/empty-states": ["libs/shared/ui/empty-states/src/index.ts"],
      "@schaeffler/file-drop": ["libs/shared/ui/file-drop/src/index.ts"],
      "@schaeffler/footer": ["libs/shared/ui/footer/src/index.ts"],
      "@schaeffler/header": ["libs/shared/ui/header/src/index.ts"],
      "@schaeffler/http": ["libs/shared/utils/http/src/index.ts"],
      "@schaeffler/icons": ["libs/shared/ui/icons/src/index.ts"],
      "@schaeffler/legal-pages": ["libs/shared/ui/legal-pages/src/index.ts"],
      "@schaeffler/loading-spinner": [
        "libs/shared/ui/loading-spinner/src/index.ts"
      ],
      "@schaeffler/picture-card": ["libs/shared/ui/picture-card/src/index.ts"],
      "@schaeffler/report": ["libs/shared/ui/report/src/index.ts"],
      "@schaeffler/responsive": ["libs/shared/utils/responsive/src/index.ts"],
      "@schaeffler/roles-and-rights": [
        "libs/shared/ui/roles-and-rights/src/index.ts"
      ],
      "@schaeffler/search-autocomplete": [
        "libs/shared/ui/search-autocomplete/src/index.ts"
      ],
      "@schaeffler/share-button": ["libs/shared/ui/share-button/src/index.ts"],
      "@schaeffler/shared/cypress-utils": [
        "libs/shared/cypress-utils/src/index.ts"
      ],
      "@schaeffler/shared/ui/storybook": [
        "libs/shared/ui/storybook/src/index.ts"
      ],
      "@schaeffler/sidebar": ["libs/shared/ui/sidebar/src/index.ts"],
      "@schaeffler/snackbar": ["libs/shared/ui/snackbar/src/index.ts"],
      "@schaeffler/stepper": ["libs/shared/ui/stepper/src/index.ts"],
      "@schaeffler/styles": ["libs/shared/ui/styles/src/index.ts"],
      "@schaeffler/subheader": ["libs/shared/ui/subheader/src/index.ts"],
      "@schaeffler/transloco": ["libs/shared/utils/transloco/src/index.ts"],
      "@schaeffler/transloco/testing": [
        "libs/shared/utils/transloco/testing/src/index.ts"
      ],
      "jszip": ["node_modules/jszip/dist/jszip.min.js"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "tmp"],
  "files": ["./.storybook/typings.d.ts"]
}
