{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/ui/storybook/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/shared/ui/storybook/.storybook",
        "browserTarget": "shared-ui-storybook:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputDir": "dist/storybook/shared-ui-storybook",
        "configDir": "libs/shared/ui/storybook/.storybook",
        "browserTarget": "shared-ui-storybook:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "standard-version": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npx standard-version --no-verify {args.params}",
        "cwd": "libs/shared/ui/storybook"
      }
    }
  },
  "tags": []
}
